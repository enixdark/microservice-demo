{
  consul: "consul:8500",
  logging: {
    "level": "INFO",
    "format": "text",
  },
  jobs: [
    {
      name: "kong",
      exec: [
        "kong", "start", "-vv"
      ],
      health: {
        exec: "kong health | grep nginx | grep running",
        interval: 10,
        ttl: 25
      }
    },
    {
      name: "kong-8000",
      port: 8000,
      tags: [
        "kong-8000",
      ],
      health: {
        exec: "/usr/bin/curl --fail -s -o /dev/null http://localhost:8000/",
        interval: 5,
        ttl: 10,
        timeout: "5s",
      }
    },
    {
      name: "kong-8443",
      port: 8443,
      tags: [
        "kong-8443",
      ],
      health: {
        exec: "/usr/bin/curl --fail -s -o /dev/null https://localhost:8443/ --insecure",
        interval: 5,
        ttl: 10,
        timeout: "5s",
      }
    },
    {
      name: "kong-8001",
      port: 8001,
      tags: [
        "kong-8001",
      ],
      health: {
        exec: "/usr/bin/curl --fail -s -o /dev/null http://localhost:8001/status",
        interval: 5,
        ttl: 10,
        timeout: "5s",
      }
    },
    {
      name: "kong-8444",
      port: 8444,
      tags: [
        "kong-8444",
      ],
      health: {
        exec: "/usr/bin/curl --fail -s -o /dev/null https://localhost:8444/ --insecure",
        interval: 5,
        ttl: 10,
        timeout: "5s",
      }
    }
  ]
}